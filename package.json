    "eslint-plugin-react-refresh": "^0.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.4.5",
    "vite": "^5.3.4",
    "vitest": "^2.0.3"
  },
  "overrides": {
    "resend": {
      "@react-email/render": "0.0.16-canary.0"
    }
  }
