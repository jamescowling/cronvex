    "cron-parser": "^4.9.0",
    "date-fns": "^3.6.0",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.408.0",
    "oslo": "^1.2.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "resend": "^3.3.0",
